# Function to search for files or directories in specified directories
search_file_or_dir() {
    selected=$(find ~/42-Code-Odyssey ~/Projects/ ~/42repo ~/Documents ~/ -mindepth 1 -maxdepth 3 | fzf)
    if [ -n "$selected" ]; then
        if [ -d "$selected" ]; then
            cd "$selected" || return  # Change to the selected directory
        elif [ -f "$selected" ]; then
            ~/nvim.appimage "$selected"  # Open the selected file
        fi
    else
        echo "No selection made."
    fi
}


